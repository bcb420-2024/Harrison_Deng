@article{rolingTwoColorHaploidGenetic2021,
  title = {A {{Two-Color Haploid Genetic Screen Identifies Novel Host Factors Involved}} in {{HIV-1 Latency}}},
  author = {Röling, Michael and Mollapour Sisakht, Mahsa and Ne, Enrico and Moulos, Panagiotis and Crespo, Raquel and Stoszko, Mateusz and De Crignis, Elisa and Bodmer, Helen and Kan, Tsung Wai and Akbarzadeh, Maryam and Harokopos, Vaggelis and Hatzis, Pantelis and Palstra, Robert-Jan and Mahmoudi, Tokameh},
  date = {2021-12-21},
  journaltitle = {mBio},
