  author = {Luo, Jinquan and Obmolova, Galina and Malia, Thomas J. and Wu, Sheng-Jiun and Duffy, Karen E. and Marion, James D. and Bell, Jessica K. and Ge, Peng and Zhou, Z. Hong and Teplyakov, Alexey and Zhao, Yonghong and Lamb, Roberta J. and Jordan, Jarrat L. and San Mateo, Lani R. and Sweet, Raymond W. and Gilliland, Gary L.},
