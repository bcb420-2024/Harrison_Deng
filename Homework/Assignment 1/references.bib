  author = {Pymm, Phillip and Tenzer, Stefan and Wee, Edmund and Weimershaus, Mirjana and Burgevin, Anne and Kollnberger, Simon and Gerstoft, Jan and Josephs, Tracy M. and Ladell, Kristin and McLaren, James E. and Appay, Victor and Price, David A. and Fugger, Lars and Bell, John I. and Schild, Hansjörg and family=Endert, given=Peter, prefix=van, useprefix=true and Harkiolaki, Maria and Iversen, Astrid K. N.},
