@article{miyamotoSARSCoV2ORF6Disrupts2022,
  title = {{{SARS-CoV-2 ORF6}} Disrupts Nucleocytoplasmic Trafficking to Advance Viral Replication},
  author = {Miyamoto, Yoichi and Itoh, Yumi and Suzuki, Tatsuya and Tanaka, Tomohisa and Sakai, Yusuke and Koido, Masaru and Hata, Chiaki and Wang, Cai-Xia and Otani, Mayumi and Moriishi, Kohji and Tachibana, Taro and Kamatani, Yoichiro and Yoneda, Yoshihiro and Okamoto, Toru and Oka, Masahiro},
