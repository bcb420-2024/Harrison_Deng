@article{sefickGenotypeRIntegratedPackage2018,
  title = {{{genotypeR}}: {{An}} Integrated r Package for Single Nucleotide Polymorphism Genotype Marker Design and Data Analysis},
  shorttitle = {{{genotypeR}}},
  author = {Sefick, Stephen A. and Castronova, Magdalena A. and Stevison, Laurie S.},
  date = {2018},
  journaltitle = {Methods in Ecology and Evolution},
