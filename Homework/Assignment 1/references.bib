@article{bishopAPOBEC3GInhibitsElongation2008,
  title = {{{APOBEC3G Inhibits Elongation}} of {{HIV-1 Reverse Transcripts}}},
  author = {Bishop, Kate N. and Verma, Mohit and Kim, Eun-Young and Wolinsky, Steven M. and Malim, Michael H.},
