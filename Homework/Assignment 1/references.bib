  shorttitle = {Chapter 3 - {{The}} Smart City in {{Singapore}}},
  booktitle = {Smart {{Cities}} for {{Technological}} and {{Social Innovation}}},
  author = {Lim, Tian Kuay and Rajabifard, Abbas and Khoo, Victor and Sabri, Soheil and Chen, Yiqun},
