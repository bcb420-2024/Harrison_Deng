@article{apanavicieneSmartBuildingIntegration2020,
  title = {Smart {{Building Integration}} into a {{Smart City}}: {{Comparative Study}} of {{Real Estate Development}}},
  shorttitle = {Smart {{Building Integration}} into a {{Smart City}}},
  author = {Apanaviciene, Rasa and Urbonas, Rokas and Fokaides, Paris A.},
  date = {2020-01},
