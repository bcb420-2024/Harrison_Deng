@online{farjoWithinhostEvolutionaryDynamics2022,
  title = {Within-Host Evolutionary Dynamics and Tissue Compartmentalization during Acute {{SARS-CoV-2}} Infection},
  author = {Farjo, Mireille and Koelle, Katia and Martin, Michael A. and Gibson, Laura L. and Walden, Kimberly K. O. and Rendon, Gloria and Fields, Christopher J. and Alnaji, Fadi G. and Gallagher, Nicholas and Luo, Chun Huai and Mostafa, Heba H. and Manabe, Yukari C. and Pekosz, Andrew and Smith, Rebecca L. and McManus, David D. and Brooke, Christopher B.},
