  author = {Manganaro, Lara and Pache, Lars and Herrmann, Tobias and Marlett, John and Hwang, Young and Murry, Jeffrey and Miorin, Lisa and Ting, Adrian T. and König, Renate and García-Sastre, Adolfo and Bushman, Frederic D. and Chanda, Sumit K. and Young, John A. T. and Fernandez-Sesma, Ana and Simon, Viviana},
  date = {2014-07},
  journaltitle = {Journal of Virology},
  shortjournal = {J Virol},
