@article{cardinaudTriggeringTLR3NOD22017,
  title = {Triggering of {{TLR-3}}, -4, {{NOD2}}, and {{DC-SIGN}} Reduces Viral Replication and Increases {{T-cell}} Activation Capacity of {{HIV-infected}} Human Dendritic Cells},
  author = {Cardinaud, Sylvain and Urrutia, Alejandra and Rouers, Angeline and Coulon, Pierre-Grégoire and Kervevan, Jérome and Richetta, Clémence and Bet, Anne and Maze, Emmanuel A. and Larsen, Martin and Iglesias, Maria-Candela and Appay, Victor and Graff-Dubois, Stéphanie and Moris, Arnaud},
  date = {2017},
  journaltitle = {European Journal of Immunology},
