@article{thornbushSmartEnergyCities2021,
  title = {Smart Energy Cities: {{The}} Evolution of the City-Energy-Sustainability Nexus},
  shorttitle = {Smart Energy Cities},
  author = {Thornbush, Mary and Golubchikov, Oleg},
  date = {2021-09-01},
  journaltitle = {Environmental Development},
