@article{kimImmunopathogenesisTreatmentCytokine2021,
  title = {Immunopathogenesis and Treatment of Cytokine Storm in {{COVID-19}}},
  author = {Kim, Jae Seok and Lee, Jun Young and Yang, Jae Won and Lee, Keum Hwa and Effenberger, Maria and Szpirt, Wladimir and Kronbichler, Andreas and Shin, Jae Il},
  date = {2021-01-01},
  journaltitle = {Theranostics},
