@article{zanenSignalPeptideHydrophobicity2005,
  title = {Signal Peptide Hydrophobicity Is Critical for Early Stages in Protein Export by {{Bacillus}} Subtilis},
  author = {Zanen, Geeske and Houben, Edith N. G. and Meima, Rob and Tjalsma, Harold and Jongbloed, Jan D. H. and Westers, Helga and Oudega, Bauke and Luirink, Joen and family=Dijl, given=Jan Maarten, prefix=van, useprefix=true and Quax, Wim J.},
