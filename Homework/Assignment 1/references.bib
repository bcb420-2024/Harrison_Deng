  author = {Holst, Johanne Marie and Enemark, Marie Beck and Pedersen, Martin Bjerregaard and Lauridsen, Kristina Lystlund and Hybel, Trine Engelbrecht and Clausen, Michael Roost and Frederiksen, Henrik and Møller, Michael Boe and Nørgaard, Peter and Plesner, Trine Lindhardt and Hamilton-Dutoit, Stephen Jacques and family=Amore, given=Francesco, prefix=d’, useprefix=true and Honoré, Bent and Ludvigsen, Maja},
  date = {2021-01},
  journaltitle = {Cancers},
