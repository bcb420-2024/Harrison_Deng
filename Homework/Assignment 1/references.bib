  author = {Fuller, Gregory N. and Hess, Kenneth R. and Rhee, Chang Hun and Yung, W. K. Alfred and Sawaya, Raymond A. and Bruner, Janet M. and Zhang, Wei},
  date = {2002-01},
  journaltitle = {Brain Pathology (Zurich, Switzerland)},
