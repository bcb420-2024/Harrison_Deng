@article{eslamiSARSCoV2ReceptorCoreceptor2022,
  title = {{{SARS-CoV-2}}: {{Receptor}} and {{Co-receptor Tropism Probability}}},
  shorttitle = {{{SARS-CoV-2}}},
  author = {Eslami, Narges and Aghbash, Parisa Shiri and Shamekh, Ali and Entezari-Maleki, Taher and Nahand, Javid Sadri and Sales, Abolfazl Jafari and Baghi, Hossein Bannazadeh},
