@article{gaiarsaComparativeAnalysisSARSCoV22022,
  title = {Comparative Analysis of {{SARS-CoV-2}} Quasispecies in the Upper and Lower Respiratory Tract Shows an Ongoing Evolution in the Spike Cleavage Site},
  author = {Gaiarsa, Stefano and Giardina, Federica and Batisti Biffignandi, Gherard and Ferrari, Guglielmo and Piazza, Aurora and Tallarita, Monica and Novazzi, Federica and Bandi, Claudio and Paolucci, Stefania and Rovida, Francesca and Campanini, Giulia and Piralla, Antonio and Baldanti, Fausto},
